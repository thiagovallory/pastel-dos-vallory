{
  "name": "pastel-dos-vallory",
  "version": "1.0.0",
  "description": "Sistema de pedidos de past√©is",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:host": "concurrently \"npm run dev:backend\" \"npm run dev:frontend:host\"",
    "dev:backend": "cd backend && node index.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:frontend:host": "cd frontend && npm run dev -- --host",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && node index.js"
  },
  "keywords": ["pastel", "pedidos", "react", "node"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 